        <filter-class>com.guohaoshiye.yueba.session.CheckSession</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CheckSession</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/error/404.jsp</location>
    </error-page>
    <error-page>
