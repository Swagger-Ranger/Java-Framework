<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.guohaoshiye.yueba.entity.ProxyNum" table="proxy_num">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
      
        <property name="yiji" type="java.lang.Integer">
            <column name="yiji">
                <comment>一级权限人数</comment>
            </column>
        </property>
        <property name="erji" type="java.lang.Integer">
            <column name="erji">
                <comment>二级权限人数</comment>
            </column>
        </property>
         <property name="sanji" type="java.lang.Integer">
            <column name="sanji">
                <comment>三级权限人数</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>