<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.guohaoshiye.yueba.entity.EmailType" table="email_type" schema="yueba">
        <id name="id" column="id"/>
        <property name="mailTypeId" column="mail_type_id"/>
        <property name="mailTime" column="mail_time"/>
        <property name="isSame" column="is_same"/>
        <property name="isTop" column="is_top"/>
        <property name="canDelete" column="can_delete"/>
    </class>
</hibernate-mapping>