<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.guohaoshiye.yueba.entity.Proxy_class" table="proxy_class" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="pid" type="java.lang.Integer">
            <column name="pid">
                <comment>用户ID</comment>
            </column>
        </property>
      
        <property name="clazz" type="java.lang.Integer">
            <column name="clazz">
                <comment>级别</comment>
            </column>
        </property>
        <property name="times" type="java.lang.Long">
            <column name="times">
                <comment>创建时间</comment>
            </column>
        </property>
         <property name="yikai" type="java.lang.Integer">
            <column name="yikai">
                <comment>已经开权限人数</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>